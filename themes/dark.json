{
  "name": "Monokai Minimal",
  "type": "dark",
  "colors": {
    "focusBorder": "#62d8f1",
    "foreground": "#af87ff",
    "activityBar.background": "#121212",
    "activityBar.border": "#424d66",
    "activityBar.foreground": "#f92672",
    "breadcrumb.foreground": "#ffb140",
    "breadcrumb.focusForeground": "#f92672",
    "diffEditor.insertedTextBackground": "#28a74530",
    "diffEditor.removedTextBackground": "#d73a4930",
    "diffEditor.diagonalFill": "#d73a4930",
    "editorCursor.foreground": "#f92672",
    "editor.background": "#1a1a1a",
    "editor.findMatchBackground": "#641313",
    "editor.findMatchHighlightBackground": "#5e2c3e",
    "editor.selectionBackground": "#590f29",
    "editor.selectionHighlightBackground": "#463763",
    "editorGroupHeader.tabsBackground": "#121212",
    "gitDecoration.addedResourceForeground": "#a4e400",
    "gitDecoration.modifiedResourceForeground": "#62d8f1",
    "gitDecoration.deletedResourceForeground": "#f92672",
    "gitDecoration.untrackedResourceForeground": "#a4e400",
    "gitDecoration.ignoredResourceForeground": "#959da5",
    "gitDecoration.conflictingResourceForeground": "#ff9700",
    "gitDecoration.submoduleResourceForeground": "#959da5",
    "input.background": "#1a1a1a",
    "input.border": "#62d8f1",
    "input.foreground": "#a4e400",
    "inputOption.activeBorder": "#f92672",
    "list.activeSelectionBackground": "#323334",
    "list.activeSelectionForeground": "#af87ff",
    "list.highlightForeground": "#f92672",
    "list.hoverBackground": "#424d66",
    "list.hoverForeground": "#a4e400",
    "list.inactiveSelectionBackground": "#1a1a1a",
    "list.inactiveSelectionForeground": "#af87ff",
    "scrollbarSlider.background": "#424d66",
    "sideBar.background": "#121212",
    "sideBar.border": "#424d66",
    "sideBar.foreground": "#af87ff",
    "sideBarSectionHeader.background": "#262626",
    "sideBarSectionHeader.border": "#424d66",
    "sideBarSectionHeader.foreground": "#959da5",
    "sideBarTitle.foreground": "#f92672",
    "statusBar.foreground": "#959da5",
    "statusBar.background": "#121212",
    "statusBar.border": "#424d66",
    "statusBarItem.hoverBackground": "#424d66",
    "tab.activeBackground": "#1a1a1a",
    "tab.activeForeground": "#af87ff",
    "tab.inactiveBackground": "#121212",
    "terminal.background": "#1a1a1a",
    "terminal.foreground": "#af87ff",
    "terminalCursor.background": "#ff9700",
    "terminalCursor.foreground": "#ff9700",
    "titleBar.activeBackground": "#121212"
  },
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#5f5f5f"
      }
    },
    {
      "scope": "constant",
      "settings": {
        "foreground": "#af87ff"
      }
    },
    {
      "scope": "entity",
      "settings": {
        "foreground": "#62d8f1"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#a4e400"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#f92672"
      }
    },
    {
      "scope": "entity.name.section",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.other.ainherited-class"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#5f5f5f",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#f92672"
      }
    },
    {
      "scope": "markup",
      "settings": {
        "foreground": "#ebebe4"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.fenced_code.block",
        "markup.inline.raw.string"
      ],
      "settings": {
        "foreground": "#b3b3b3"
      }
    },
    {
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#62d8f1"
      }
    },
    {
      "scope": "fenced_code.block.language.markdown",
      "settings": {
        "foreground": "#af87ff"
      }
    },
    {
      "scope": [
        "meta",
        "meta.function"
      ],
      "settings": {
        "foreground": "#a4e400"
      }
    },
    {
      "scope": [
        "meta.class",
        "meta.line"
      ],
      "settings": {
        "foreground": "#62d8f1"
      }
    },
    {
      "scope": "meta.objectliteral",
      "settings": {
        "foreground": "#f92672"
      }
    },
    {
      "scope": "meta.object-literal.key",
      "settings": {
        "foreground": "#af87ff"
      }
    },
    {
      "scope": "meta.paragraph",
      "settings": {
        "foreground": "#ebebe4"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#f92672"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#ffff87"
      }
    },
    {
      "scope": [
        "support.function",
        "support.variable",
        "support.other"
      ],
      "settings": {
        "foreground": "#62f1a2",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "support.class",
        "support.type",
        "support.function.kernel"
      ],
      "settings": {
        "foreground": "#62d8f1",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#af87ff",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#ffb957"
      }
    },
    {
      "scope": [
        "variable.parameter",
        "variable.language"
      ],
      "settings": {
        "foreground": "#ff9700",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "variable.other.constant",
      "settings": {
        "foreground": "#62d8f1"
      }
    },
    {
      "scope": "variable.other.readwrite.instance",
      "settings": {
        "foreground": "#ffb140"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#ffff87"
      }
    },
    {
      "scope": "punctuation.section",
      "settings": {
        "foreground": "#ffe100"
      }
    },
    {
      "scope": [
        "punctuation.definition.constant.hashkey",
        "punctuation.definition.heading",
        "punctuation.definition.keyword",
        "punctuation.definition.list",
        "punctuation.definition.raw",
        "punctuation.definition.markdown",
        "punctuation.section.embedded",
        "punctuation.separator.dictionary.key-value",
        "punctuation.separator.key-value",
        "punctuation.separator.namespace"
      ],
      "settings": {
        "foreground": "#f92672"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "punctuation.definition.variable",
        "punctuation.definition.bold",
        "punctuation.definition.italic"
      ],
      "settings": {
        "foreground": "#ff9700",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "punctuation.definition.constant",
        "punctuation.separator.method",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#af87ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#b3b3b3"
      }
    },
    {
      "scope": "punctuation.separator.inheritance",
      "settings": {
        "foreground": "#a4e400"
      }
    },
    {
      "scope": [
        "punctuation.support",
        "punctuation.definition.entity",
        "punctuation.definition.metadata"
      ],
      "settings": {
        "foreground": "#62d8f1"
      }
    }
  ]
}