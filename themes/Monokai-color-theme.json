{
  "name": "Monokai",
  "type": "dark",
  "colors": {
    "editor.background": "#1a1a1a",
    "editor.selectionBackground": "#590f29",
    "editor.selectionHighlightBackground": "#463763",
    "editor.findMatchBackground": "#641313",
    "editor.findMatchHighlightBackground": "#5e2c3e",
    "activityBar.background": "#121212",
    "activityBar.border": "#424d66",
    "activityBar.foreground": "#f92672",
    "sideBar.background": "#121212",
    "sideBar.border": "#424d66",
    "sideBar.foreground": "#af87ff",
    "sideBarTitle.foreground": "#f92672",
    "sideBarSectionHeader.foreground": "#969896",
    "sideBarSectionHeader.background": "#262626",
    "sideBarSectionHeader.border": "#424d66",
    "scrollbarSlider.background": "#424d66",
    "gitDecoration.untrackedResourceForeground": "#a4e400",
    "gitDecoration.modifiedResourceForeground": "#e0d75a",
    "titleBar.activeBackground": "#121212",
    "tab.inactiveBackground": "#121212",
    "tab.activeBackground": "#1a1a1a",
    "tab.activeForeground": "#af87ff",
    "list.hoverBackground": "#424d66",
    "list.hoverForeground": "#a4e400",
    "list.inactiveSelectionBackground": "#1a1a1a",
    "list.inactiveSelectionForeground": "#af87ff",
    "list.activeSelectionBackground": "#323334",
    "list.activeSelectionForeground": "#a17cff",
    "list.highlightForeground": "#f92672",
    "input.background": "#1a1a1a",
    "input.foreground": "#a4e400",
    "input.border": "#62d8f1",
    "inputOption.activeBorder": "#f92672",
    "statusBar.foreground": "#969896",
    "statusBar.background": "#121212",
    "statusBar.border": "#424d66",
    "statusBarItem.hoverBackground": "#424d66",
    "editorGroupHeader.tabsBackground": "#121212",
    "terminal.background": "#1a1a1a",
    "terminal.foreground": "#a17cff",
    "foreground": "#a17cff",
    "breadcrumb.foreground": "#fd971f"
  },

  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#5f5f5f"
      }
    },


// Constant
    {
      "scope": "constant",
      "settings": {
        "foreground": "#af87ff"
      }
    },


// Entity
    {
      "scope": "entity",
      "settings": {
        "foreground": "#62d8f1"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#a4e400"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#f92672"
      }
    },
    {
      "scope": "entity.name.section",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },


// Invalid
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#5f5f5f",
        "fontStyle": "italic"
      }
    },


// Keyword
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#f92672"
      }
    },


// Markup
    {
      "scope": "markup",
      "settings": {
        "foreground": "#ebebe4"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.fenced_code.block",
        "markup.inline.raw.string"
      ],
      "settings": {
        "foreground": "#b3b3b3",
        
      }
    },
    {
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#62d8f1",
      }
    },
    {
      "scope": "fenced_code.block.language.markdown",
      "settings": {
        "foreground": "#af87ff",
      }
    },


// Meta
    {
      "scope": "meta",
      "settings": {
        "foreground": "#a4e400"
      }
    },
    {
      "scope": [
        "meta.class",
        "meta.line"
      ],
      "settings": {
        "foreground": "#62d8f1"
      }
    },
    {
      "scope": "meta.function",
      "settings": {
        "foreground": "#a4e400"
      }
    },
    {
      "scope": "meta.objectliteral",
      "settings": {
        "foreground": "#f92672"
      }
    },
    {
      "scope": "meta.object-literal.key",
      "settings": {
        "foreground": "#af87ff"
      }
    },
    {
      "scope": "meta.paragraph",
      "settings": {
        "foreground": "#ebebe4"
      }
    },


// Storage
    {
      "scope": "storage",
      "settings": {
        "foreground": "#f92672"
      }
    },


// String
    {
      "scope": "string",
      "settings": {
        "foreground": "#ffff87"
      }
    },


// Support
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#a4e400",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "support.class",
        "support.type",
        "support.function.kernel"
      ],
      "settings": {
        "foreground": "#62d8f1",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#af87ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "support.variable",
        "support.other"
      ],
      "settings": {
        "foreground": "#ffb957",
        "fontStyle": "bold"
      }
    },


// Variable
    {
      "scope": "variable",
      "settings": {
        "foreground": "#ffb957"
      }
    },
    {
      "scope": [
        "variable.parameter",
        "variable.language",
      ],
      "settings": {
        "foreground": "#ff9700",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "variable.other.constant",
      "settings": {
        "foreground": "#62d8f1"
      }
    },
    {
      "scope": "variable.other.readwrite.instance",
      "settings": {
        "foreground": "#ffb140"
      }
    },



// Punctuation
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#ffff87"
      }
    },
    {
      "scope": [
        "punctuation.definition.constant.hashkey",
        "punctuation.definition.heading",
        "punctuation.definition.keyword",
        "punctuation.definition.list",
        "punctuation.definition.raw",
        "punctuation.definition.markdown",
        "punctuation.section",
        "punctuation.section.embedded",
        "punctuation.separator.dictionary.key-value",
        "punctuation.separator.key-value",
        "punctuation.separator.namespace",
      ],
      "settings": {
        "foreground": "#f92672"
      }
    },
    {
      "scope": "punctuation.definition.string",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "punctuation.definition.variable",
        "punctuation.definition.bold",
        "punctuation.definition.italic"
      ],
      "settings": {
        "foreground": "#ff9700",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "punctuation.definition.constant",
        "punctuation.separator.method",
        "punctuation.accessor",
      ],
      "settings": {
        "foreground": "#af87ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#f8f8f2"
      }
    },
    {
      "scope": "punctuation.separator.inheritance",
      "settings": {
        "foreground": "#a4e400"
      }
    },
    {
      "scope": [
        "punctuation.support",
        "punctuation.definition.entity",
        "punctuation.definition.metadata",
      ],
      "settings": {
        "foreground": "#62d8f1"
      }
    }
  ]
}
